ul.nav.nav-pills.pull-right(role="tablist")
  li(class=(title === 'Guest Logs') ? 'active' : undefined, role="presentation")
    a(href="/dashboard/guest-logs") Guest Logs
  li(class=(title === 'Manage Non-Members') ? 'active' : undefined, role="presentation")
    a(href="/dashboard") Manage Non-Members
  li(class=(title.indexOf('Edit Form') > -1) ? 'active' : undefined, role="presentation")
    a(href="/dashboard/edit") Edit Form
  li(class=(title === 'Change Password') ? 'active' : undefined, role="presentation")
    a(href="/dashboard/change-password") Change Password
